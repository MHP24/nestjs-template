  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "9.27.0",
    "@nestjs/cli": "11.0.7",
    "@nestjs/schematics": "11.0.5",
    "@types/express": "5.0.2",
    "@types/node": "22.15.21",
